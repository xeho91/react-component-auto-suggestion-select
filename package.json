{
	"$schema": "https://json.schemastore.org/package",
	"private": true,
	"type": "module",
	"sideEffects": false,
	"engines": {
		"node": ">=16",
		"pnpm": ">=6"
	},
	"packageManager": "pnpm@6.32.3",
	"volta": {
		"node": "16.14.2"
	},
	"scripts": {
		"browsers": "browserslist",
		"build": "ladle build",
		"clean": "concurrently \"pnpm:clean:*\" --group --timings",
		"clean:build": "del-cli \"./build\"",
		"clean:cache": "del-cli \"./node_modules/.cache\"",
		"coverage": "vitest run --coverage",
		"dev": "ladle serve",
		"format": "pretty-quick --branch main --pattern \"**/*\" --verbose",
		"lint": "concurrently \"pnpm:lint:*\" --group --timings",
		"lint:format": "pretty-quick --branch main --pattern \"**/*\" --verbose --check",
		"lint:css": "DEBUG=\"stylelint:*\" stylelint \"./source/**/*.scss\" --cache --cache-location \"./node_modules/.cache/stylelint\"",
		"lint:js": "DEBUG=\"eslint:cli-engine\" eslint . --ext .json,.ts,.tsx --cache --cache-location \"./node_modules/.cache/eslint\"",
		"lint:md": "markdownlint . --dot --ignore-path \"./.gitignore\"",
		"lint:types": "tsc --noEmit",
		"serve": "serve \"./build\"",
		"test": "vitest run",
		"test:dev": "vitest watch --ui"
	},
	"dependencies": {
		"@faker-js/faker": "6.1.1",
		"@iconify/react": "3.2.0",
		"axios": "0.26.1",
		"clsx": "1.1.1",
		"framer-motion": "6.2.8",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-error-boundary": "3.1.4",
		"react-query": "3.34.19",
		"react-tracked": "1.7.8",
		"react-virtual": "2.10.4",
		"zustand": "3.7.1"
	},
	"devDependencies": {
		"@ladle/react": "0.10.1",
		"@terminal-nerds/eslint-config": "0.8.0",
		"@terminal-nerds/markdownlint-config": "0.4.0",
		"@terminal-nerds/prettier-config": "0.2.1",
		"@terminal-nerds/stylelint-config": "0.6.0",
		"@terminal-nerds/typescript-config": "0.3.0",
		"@testing-library/dom": "8.12.0",
		"@testing-library/jest-dom": "5.16.3",
		"@testing-library/react": "12.1.4",
		"@testing-library/react-hooks": "7.0.2",
		"@testing-library/user-event": "14.0.0",
		"@types/node": "17.0.23",
		"@types/react": "17.0.43",
		"@types/react-dom": "17.0.14",
		"@vitejs/plugin-react": "1.3.0",
		"@vitest/ui": "0.8.1",
		"browserslist": "4.20.2",
		"c8": "7.11.0",
		"concurrently": "7.0.0",
		"del-cli": "4.0.1",
		"eslint": "8.12.0",
		"jsdom": "19.0.0",
		"markdownlint-cli": "0.31.1",
		"msw": "0.39.2",
		"postcss": "8.4.12",
		"postcss-flexbugs-fixes": "5.0.2",
		"postcss-preset-env": "7.4.3",
		"prettier": "2.6.1",
		"pretty-quick": "3.1.3",
		"react-test-renderer": "17.0.2",
		"sass": "1.49.9",
		"stylelint": "14.6.1",
		"tailwindcss": "3.0.23",
		"typescript": "4.6.3",
		"typescript-plugin-css-modules": "3.4.0",
		"vite": "2.8.6",
		"vite-tsconfig-paths": "3.4.1",
		"vitest": "0.8.0"
	},
	"license": "MIT"
}
